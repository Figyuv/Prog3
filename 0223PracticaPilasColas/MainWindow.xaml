        <Button x:Name="btnPush" Content="Push" HorizontalAlignment="Left" Margin="71,105,0,0" VerticalAlignment="Top" Height="45" Width="71" Click="btnPush_Click"/>
        <Button x:Name="btnPop" Content="Pop" HorizontalAlignment="Left" Margin="297,117,0,0" VerticalAlignment="Top" Height="45" Width="66" Click="btnPop_Click"/>
        <ListBox x:Name="lstGarage" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="147,88,292,100"/>
        <ListBox x:Name="lstStreet" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="378,88,51,100"/>
        <TextBox x:Name="txtPlaca" HorizontalAlignment="Left" Margin="142,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="95" Height="42"/>
        <TextBox x:Name="txtColor" HorizontalAlignment="Left" Margin="253,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="77" Height="42"/>

    </Grid>
</Window>
